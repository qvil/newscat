{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [".js", ".ios.js", ".android.js", ".ts", ".tsx"],
        "alias": {
          "src": "./src",
          "components": "./src/components",
          "lib": "./src/lib",
          "img": "./img",
          "config": "./config"
        }
      }
    ]
  ]
}
